{
    "manifest_version": 2,
    "name": "消息测试1",
    "version": "1.0",
    "description": "popup和content的通信",
    "browser_action": {
        "default_title": "消息测试1"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "js/jquery.js",
                "js/content.js"
            ]
        }
    ],
    "permissions": [
        "contextMenus"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}